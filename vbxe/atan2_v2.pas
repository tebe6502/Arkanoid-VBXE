var
 f: file;
 
 scale_l, scale_h: array [0..255] of byte;
 
 i: byte;
 
 w: word;
  
  
begin

 for i:=0 to 255 do begin
  w:=round(i*(360/256));
  
  scale_l[i] := lo(w);
  scale_h[i] := hi(w);
 end;  

 assign(f, 'atan_tab2.dat'); rewrite(f, 1);
 
 blockwrite(f, atan_tab, 256);
 blockwrite(f, log2_tab, 256);
 blockwrite(f, scale_l, 256);
 blockwrite(f, scale_h, 256);
 
 close(f);

end.